    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const element = document.body;
            const themeToggle = document.querySelector('.themeMode');
            const menu = document.querySelector('nav .loginBox .menu');
            const login = document.querySelector('nav .loginBox .login');
            const isDarkMode = localStorage.getItem('theme') === 'true';
            if (isDarkMode) {
                element.classList.add('darkMode');
            }
             
            menu.addEventListener('click', () => {
                const dropDown = document.querySelector('.dropDown')
                console.log(dropDown.classList,'dropdownfreurgir');
                dropDown.classList.toggle('visible');
            });
    
            login.addEventListener('click', () => {
                const dropDown = document.querySelector('.loginDropDown')
                console.log(dropDown.classList,'dropdownfreurgir');
                dropDown.classList.toggle('visible');
            });
           
            themeToggle.addEventListener('click', () => {
                element.classList.toggle('darkMode');
                const isDarkModeNow = element.classList.contains('darkMode');
                localStorage.setItem('theme', isDarkModeNow.toString());
            });
    
    
        });
    </script>
